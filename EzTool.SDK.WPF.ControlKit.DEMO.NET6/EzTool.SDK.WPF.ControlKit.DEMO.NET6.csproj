<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<RootNamespace>EzTool.SDK.WPF.ControlKit</RootNamespace>
		<IncludePackageReferencesDuringMarkupCompilation>false</IncludePackageReferencesDuringMarkupCompilation>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="_images\defaulticon.png" />
	  <None Remove="_images\setting.jfif" />
	  <None Remove="_images\settings.png" />
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="_images\defaulticon.png" />
	  <Resource Include="_images\settings.png" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="EzTool.SDK.WPF.Unicorn.Plus" Version="1.0.4" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\EzTool.SDK.WPF.ControlKit.NET6\EzTool.SDK.WPF.ControlKit.NET6.csproj" />
	</ItemGroup>

</Project>
